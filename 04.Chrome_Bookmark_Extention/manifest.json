{
  "name": "Code Bookmarks",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Short cut bookmark for code snnippets site",

  "page_action":{
    "default_icon": "img/19_nb.png"
  },

  "icons": { 
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },

  "background":{
    "scripts":["background.js"],
    "persistent": false
  },

  "permissions": [
    "bookmarks",
    "tabs"
   ]
}